{
    "version": 2,
    "framework": "nextjs",
    "regions": [
        "bom1"
    ],
    "cleanUrls": true,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                }
            ]
        }
    ]
}