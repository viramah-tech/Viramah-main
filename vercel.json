{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "nextjs",
    "functions": {
        "src/app/api/enquiry/route.ts": {
            "maxDuration": 10,
            "regions": [
                "sin1"
            ]
        },
        "src/app/api/health/route.ts": {
            "maxDuration": 5,
            "regions": [
                "sin1"
            ]
        }
    },
    "headers": [
        {
            "source": "/amenities/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}